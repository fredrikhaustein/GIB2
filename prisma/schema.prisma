generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model fjell_over_1000m {
  ogc_fid      Int      @id @default(autoincrement())
  wkb_geometry Bytes?
  navn         String?  @db.VarChar
  lat          Float?
  lon          Float?
  h_yde        Int?     @map("h√∏yde")
  route_urls   String[]
}

model gpx_route {
  route_name  String?
  gpx_link    String
  mountain_id Int
  route_id    Int     @id @default(autoincrement())
}

model user {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  email     String?  @unique
  image     String?
  role      Role     @default(USER)
}

enum Role {
  USER
  ADMIN
}
